{
  "npmScope": "cheesecake-ui",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "cheesecake-web": {
      "tags": []
    },
    "cheesecake-web-e2e": {
      "tags": [],
      "implicitDependencies": ["cheesecake-web"]
    },
    "core-api": {
      "tags": ["type:core"]
    },
    "core-auth": {
      "tags": ["type:core"]
    },
    "features-activity-create": {
      "tags": ["type:feature"]
    },
    "features-activity-details": {
      "tags": ["type:feature"]
    },
    "features-activity-edit": {
      "tags": ["type:feature"]
    },
    "features-activity-list": {
      "tags": ["type:feature"]
    },
    "features-auth-login": {
      "tags": ["type:feature"]
    },
    "features-home": {
      "tags": ["type:feature"]
    },
    "shared-components-drawer": {
      "tags": ["type:ui"]
    },
    "test-mock": {
      "tags": ["type:test"]
    },
    "ui-application-shell": {
      "tags": ["type:ui"]
    },
    "ui-shell": {
      "tags": ["type:ui"]
    },
    "utils-form": {
      "tags": ["type:util"]
    }
  }
}
